{

    "tabWidth": 2,
    
    "useTabs":false,
    
    "semi": false,
    
    "singleQuote": true,
    
    "printWidth": 1000,
    
    "trailingComma": "none",
    
    "overrides": [
    
    {
    
    "files": [
    
    "**/*.ts",
    
    "**/*.js"
    
    ],
    
    "options": {
    
    "parser": "typescript",
    
    "singleQuote": true,
    
    "printWidth": 200
    
    }
    
    },
    
    {
    
    "files": "**/*.scss",
    
    "options": {
    
    "parser": "css"
    
    }
    
    },
    
    {
    
    "files": "**/*.json",
    
    "options": {
    
    "parser": "json",
    
    "printWidth": 1200
    
    }
    
    }
    
    ]
    
    }
    
    